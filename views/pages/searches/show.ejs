<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Google Book Search </title>
  <ul>
    <% data.forEach(value => {%>
    <h2></h2>
    <li><%= value.name %></li>
    <li><%= value.qty %></li>
  })
  </ul>
</head>
<form action="/new.ejs" method="POST">
  <form action="server.js" method="POST" class="form-example">
    <div class="form-example">
      <label for="title">Title </label>
      <input type="radio" name="name" id="name" required>
    </div>
    <div class="form-example">
      <label for="radio">Author</label>
      <input type="author" name="email" id="email" required>
    </div>
    <div class="form-example">
      <input type="submit" value="Subscribe!">
    </div>
  </form>
</form>
<body>
  
</body>
</html>